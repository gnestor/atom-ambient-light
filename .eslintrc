{
  "extends": "airbnb",
  "settings": {
    "import/core-modules": ["atom"]
  },
  "globals": {
    "atom": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "consistent-return": 0,
    "max-len": [0, 80],
    "no-unused-vars": 1,
    "no-shadow": 0,
    "arrow-parens": 0
  }
}
